<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FocusLock_Application">

        <activity android:name=".options" android:exported="true" />
        <activity android:name=".login" android:exported="true" />
        <activity android:name=".signup" android:exported="true" />
        <activity android:name=".MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- other activities -->
        <activity android:name=".parents_settasks" android:exported="false" />
        <activity android:name=".settingsverify" android:exported="false" />
        <activity android:name=".parent_setpoints" android:exported="false" />
        <activity android:name=".completionlog" android:exported="false" />
        <activity android:name=".parent_apps" android:exported="false" />
        <activity android:name=".verification" android:exported="true" />
        <activity android:name=".leaderboards" android:exported="false" />
        <activity android:name=".settings" android:exported="false" />
        <activity android:name=".task" android:exported="false" />
        <activity android:name=".appsontimeout" android:exported="true" />

    </application>

</manifest>
